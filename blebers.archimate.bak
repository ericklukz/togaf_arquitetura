  <folder name="Motivation" id="id-30262e5e11c347ab813c8f60ea833b86" type="motivation">
    <element xsi:type="archimate:Stakeholder" name="Presidente da SoftwareHouse" id="id-811a5bc2e6e04407ba433b4ce074d3ee"/>
    <element xsi:type="archimate:Driver" name="Não podem mais ocorrer atendimentos de suporte de maneira informal, pois clientes estão  cancelando contratos conosco por insatisfação com o atendimento" id="id-63513b29722a44b4aad0868cf2e3284c"/>
    <element xsi:type="archimate:Value" name="Aumentará a retenção de clientes(renovação de contratos) Perdemos em média 120mil reais de contratos cancelados por insatifação com o suporte técnico com os nossos produtos." id="id-f9fae2033d4f4341bab2449f86f26c8f"/>
    <element xsi:type="archimate:Value" name="Um cliente satisfeito amplia negócios e indica novos clientes.Ampliamos 280mil por ano do nosso faturamento com as ampliações de negócios partindo de indicações dos cliente satisfeitos." id="id-ee4a4b0d81c44dd09648bbe868e43c77"/>
    <element xsi:type="archimate:Assessment" name="Registro e acompanhamento dos atendimentos até o encerramento usando um sistema digital informatizado" id="id-339a7fbbf8e94ab785cadbb24aa35d47"/>
    <element xsi:type="archimate:Assessment" name="Ter um meio de acompanhar o grau de satisfação dos clientes com o suporte de maneira formal." id="id-4bd6898426eb4739a64bc4e18a73a963"/>
    <element xsi:type="archimate:Meaning" name="Meaning" id="id-d81645a34de04a29a1724d2de867ea75"/>
    <element xsi:type="archimate:Meaning" name="Meaning" id="id-4eaaaa8b04c240f2996c91e9a5685b0b"/>
    <element xsi:type="archimate:Meaning" name="Meaning" id="id-076882969b8146d8abc81b0e81ec2897"/>
    <element xsi:type="archimate:Goal" name="Uma informatazação da abertura de ocorrências técnicas no suporte." id="id-b2259f16c46b425a9a02e81cd649f6a3"/>
    <element xsi:type="archimate:Goal" name="Informatizar o processo de diagnóstico e solução de chamados" id="id-0efc977c1ca843b7a951845df39c6fd1"/>
    <element xsi:type="archimate:Outcome" name="Permitir que o cliente a partir de um software avaliar o processo de maneira objetiva. " id="id-8b25b37f680f48ff9e51ee91411b56d1"/>
    <element xsi:type="archimate:Goal" name="Permitir que o cliente a partir de um software avaliar o processo de maneira objetiva. " id="id-2ec2a1216eec4381b79656b315064a36"/>
    <element xsi:type="archimate:Outcome" name="100% dos chamados precisam estar registrados no novo sistema, sendo que essa solução de documentação digital de chamados precisa ser liberada até 31/12/2023." id="id-8c46d8a442194910a565888853788697"/>
    <element xsi:type="archimate:Outcome" name="O cliente deve pontuar o atendimento com notas objetivas. Esse sistema de avaliação deve estar disponível 3 meses depois da implantação do sistema de registros" id="id-bfad1b5a3ce9448d9c6cbb1a7cd8b073"/>
    <element xsi:type="archimate:Principle" name="Não obrigar o cliente a responder a pesquisa avaliativa de satisfação. A avaliação é opcional." id="id-99f8c1c4efbe4fb0b3a6458c5368347e"/>
    <element xsi:type="archimate:Principle" name="Ao solucionar um chamado a pesquisa de satisfação deve ser disparada para o cliente." id="id-c8287be639474e5f99e2a405c51b5132"/>
    <element xsi:type="archimate:Principle" name="Recepcionar os chamados direcionando imediatamente para uma equipe especializada no tipo de problema que o cliente enfrenta" id="id-258b8d0d91df44b389a09486ed58b1bb"/>
    <element xsi:type="archimate:Principle" name="Prover uma base de conhecimento sobre problemas e soluções já catalogados para agilizar a solução" id="id-036ce892f32b4db7989191ab54ef61d0"/>
  </folder>
